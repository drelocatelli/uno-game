{
  "name": "whatsflow-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "module": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsnd -r dotenv/config --respawn --exit-child --transpile-only --watch --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "start": "node -r dotenv/config dist/server.js",
    "db:migrations": "npx prisma migrate dev --preview-feature",
    "db:reset": "npx prisma migrate reset --preview-feature",
    "db:migrate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:legacy": "npx prisma introspect",
    "formatter": "npx prettier --write src/**/*.ts",
    "pull": "git pull && npm i -D && npx prisma generate && npx prisma db push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esm": "^3.2.25",
    "gts": "^3.1.0",
    "prettier": "^2.6.1",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "cron": "^1.8.2",
    "dotenv": "^16.0.0",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "socket-controllers": "^0.1.2",
    "socket.io": "^4.6.1",
    "typedi": "^0.10.0"
  }
}
